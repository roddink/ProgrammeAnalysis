program 
  int x;
  int y;
  x := 10; (* L1 *)
  y := 5;  (* L2 *)
  if(x<5)  (* L3 *)
    then y := x-y; (* L4 *)
    else y := 10;  (* L5 *)
  fi
  write y; (* real: y = {L5, L3, L1}, expected deduction: y = {L5, L4, L3, L2, L1} *)
end
  
