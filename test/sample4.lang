program
int n;
int x;
int f1;
int f2;
int ans;
n := 20;
f1 := 0;
f2 := 1;
x := 2;
ans := 0;
while x <= n do 
	f1:=1;
	f2:=3;
	if ans > -1 then
		ans:=1;
		f1:=2;
	else
		ans:=2;
		f2:=2;
	fi
	ans := f1 + f2;
	f1 := f2;
	f2 := ans;		(* The point of interest *)
	x := x + 1;
od
end
