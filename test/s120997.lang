(* Example program for program slicing *)
program
	int x;
	int y;
	x := 2; (* This line should be removed by slicing *)
	x := 5;
	y := 2; (* This line should be removed by slicing *)
	while (x < 10)
	do
		x := x + 1;
		y := y + 1; (* This line should be removed by slicing *)
	od
	write x; (* Point of interest *)
end